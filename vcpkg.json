{
  "name": "rabbitremotecontrol",
  "version-string":"0.0.23",

  "dependencies": [
      "openssl",
      "zlib"
  ],

  "default-features": ["freerdp"],

  "features": {
      "client": {
          "description": "client",
          "dependencies": [
              "freerdp"
          ]
      },

      "freerdp": {
          "description": "freerdp",
          "dependencies": [
              "libdatachannel",
              {
                  "name": "freerdp",
                  "platform": "windows | osx | ios | linux | mingw"
              }
          ]
      },

      "vnc": {
          "description": "vnc",
          "dependencies": [
              "libjpeg-turbo",
              "pixman"
          ]
      },
      
      "terminal": {
          "description": "terminal",
          "dependencies": [
              {
                  "name": "libssh"
              }
          ]
      }
  },

  "builtin-baseline": "9259a0719d94c402aae2ab7975bc096afdec15df",
  "overrides": [
      { "name": "openssl", "version-string": "1.1.1n" }
  ]
}
